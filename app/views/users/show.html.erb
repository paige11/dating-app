<h1>Welcome, <%= @user.username %></h1>

<h3>Your Personality Type: <%= link_to @user.personality.type, @user.personality_type.url, :target => "_blank" %></h3>

<h3>Your Matches</h3>
<% if @matches %>
  <% @matches.each do |match| %>
    <%= link_to match.partner.username, user_path(match.partner) %>
  <% end %>
<% end %>
